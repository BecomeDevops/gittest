pipeline {

  agent any 

  stages {

stage('git clone') {

steps {

echo "cloning the repository code"
  
}


  }


    stage('Build') {

steps {

echo "build the code using maven tool"
  
}


  }

    stage('push to artifactory') {

steps {

echo "push the artificat to nexus"
  
}


  }





    
  }




}
